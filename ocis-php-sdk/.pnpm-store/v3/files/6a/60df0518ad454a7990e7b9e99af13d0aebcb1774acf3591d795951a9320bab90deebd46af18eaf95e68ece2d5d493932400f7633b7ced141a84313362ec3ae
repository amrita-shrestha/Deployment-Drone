import _extends from "@babel/runtime/helpers/extends";
import _objectWithoutProperties from "@babel/runtime/helpers/objectWithoutProperties";
import _defineProperty from "@babel/runtime/helpers/defineProperty";
import React from 'react';
import PropTypes from 'prop-types';
import classNames from 'classnames';
import { withStyles } from '@material-ui/core/styles';

var styles = function styles(theme) {
  var _root;

  return {
    root: (_root = {
      marginTop: 56
    }, _defineProperty(_root, "".concat(theme.breakpoints.up('xs'), " and (orientation: landscape)"), {
      marginTop: 48
    }), _defineProperty(_root, theme.breakpoints.up('sm'), {
      marginTop: 56
    }), _root)
  };
};

var TopBarBound = React.forwardRef(function TopBarBound(props, ref) {
  var children = props.children,
      classes = props.classes,
      classNameProp = props.className,
      other = _objectWithoutProperties(props, ["children", "classes", "className"]);

  return React.createElement("div", _extends({
    className: classNames(classes.root, classNameProp),
    ref: ref
  }, other), children);
});
TopBarBound.propTypes = undefined !== "production" ? {
  /**
     * The content of the component.
     */
  children: PropTypes.node,

  /**
   * Useful to extend the style applied to components.
   */
  classes: PropTypes.object.isRequired,

  /**
   * @ignore
   */
  className: PropTypes.string
} : {};
export default withStyles(styles, {
  name: 'KpopTopBarBound'
})(TopBarBound);